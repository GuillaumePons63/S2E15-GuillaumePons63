{
    "name": "admin-client",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite --host",
        "build": "vite build",
        "preview": "vite preview --port 4173"
    },
    "dependencies": {
        "axios": "^1.1.3",
        "passport": "^0.6.0",
        "passport-github2": "^0.1.12",
        "pinia": "^2.0.21",
        "trix": "^2.0.0-beta.0",
        "vue": "^3.2.38",
        "vue-router": "^4.1.5"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^3.0.3",
        "vite": "^3.0.9"
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "overrides": [
            {
                "files": "*.js",
                "options": {
                    "parser": "babel"
                }
            },
            {
                "files": "*.json",
                "options": {
                    "parser": "json",
                    "tabWidth": 4
                }
            }
        ],
        "semi": true,
        "requirePragma": false,
        "proseWrap": "preserve",
        "arrowParens": "avoid",
        "htmlWhitespaceSensitivity": "css",
        "quoteProps": "as-needed"
    }
}
